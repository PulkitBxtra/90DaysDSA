#include<iostream>
using namespace std;

int main(){

    int n;
    cin>>n;
    int arr[n];

    for(int i=0;i<n;i++){
        cin>>arr[i];
    }

    int i=0;
    int m=n;    
    if(n%2!=0){
        m=n-1;
    }

    while(i<m){
        
        swap(arr[i],arr[i+1]);
        i++;
        i++;
    }

    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }

   
 


}

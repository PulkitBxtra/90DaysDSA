#include<iostream>
using namespace std;

int sq(int n){
    int s=0;
    int e=n;
    long long mid=(s+e)/2;
    int ans=-1;
    while(s<=e){

        if(mid*mid==n){
            return mid;
        }

        if(mid*mid<n){
            ans=mid;
            s=mid+1;
        }

        else{
            e=mid-1;
        }

        mid=(s+e)/2;

    }
    return ans;

}


int main(){
    int n;
    cin>>n;

    cout<<sq(n)<<endl;
}
